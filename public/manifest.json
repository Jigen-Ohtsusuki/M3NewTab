{
  "manifest_version": 3,
  "name": "My Material Tab",
  "version": "0.1.0",
  "description": "A custom Material You new tab built with React.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "identity",
    "storage"
  ],
  "host_permissions": [
    "https://suggestqueries.google.com/",
    "https://www.googleapis.com/"
  ],
  "oauth2": {
    "client_id": "811527139375-f8oj1psvd3rh5c7n6ejvpi0ae75gphdk.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  }
}